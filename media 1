using System;


  class Program {
  public static void Main(string[] args) {
      
    double nota1 = double.Parse(Console.ReadLine());
    double peso1 = double.Parse(Console.ReadLine());
    double nota2 = double.Parse(Console.ReadLine());
    double peso2 = double.Parse(Console.ReadLine());

   double coiso1 =  nota1*peso1;
   double coiso2 =  nota2*peso2;
   double soma1 = coiso1+coiso2;
   double soma2 =  peso1+peso2;
     
    Console.WriteLine($"MEDIA = {soma1/soma2:0.00000}");
  }
  }
